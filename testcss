	/* Mozilla Firefox */ 
	background-image: -moz-linear-gradient(top left, #321B0F 0%, #65544B 100%) !important;
	
	/* Opera */ 
	background-image: -o-linear-gradient(top left, #321B0F 0%, #65544B 100%) !important;
	
	/* Webkit (Safari/Chrome 10) */ 
	background-image: -webkit-gradient(linear, left top, right bottom, color-stop(0, #321B0F), color-stop(1, #65544B)) !important;
	
	/* Webkit (Chrome 11+) */ 
	background-image: -webkit-linear-gradient(top left, #321B0F 0%, #65544B 100%) !important;
	
	/* W3C Markup, IE10 Release Preview */ 
	background-image: linear-gradient(to bottom right, #321B0F 0%, #65544B 100%) !important;
	
